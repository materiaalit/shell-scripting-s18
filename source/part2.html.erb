  int hyvaksytytLuvut = 0;
  int epakelvotLuvut = 0;

  while (true) {
      int luettu = Integer.valueOf(lukija.nextLine());

      if (luettu == 0) {
          System.out.println("Hyväksyttävien lukujen summa: " + summa);
          System.out.println("Hyväksyttyjä lukuja: " + hyvaksytytLuvut);
